{
 
  "compilerOptions": {
    "module":"CommonJS",
    "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "noEmit": false,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "target":"ES2019",
        "outDir": "./build"
  
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": ["**/test/**/*", "**/tests/**/*", "node_modules"]

}
