{
  "name": "graphql-testing",
  "version": "1.0.0",
  "license": "MIT",
  "description": "fork of Shopify's graphql-testing library to support new versions of Apollo Client",
  "main": "build/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "author": "John Teague",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "graphql": ">14.0.0",
    "jest-matcher-utils": "^26.6.2"
  },
  "devDependencies": {
    "@shopify/react-testing": "^2.3.5",
    "@shopify/useful-types": "^2.4.2",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.6",
    "graphql-typed": "^0.7.2",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "files": [
    "build/*",
    "!*.tsbuildinfo",
    "index.js",
    "index.mjs",
    "index.esnext",
    "index.d.ts",
    "matchers.js",
    "matchers.mjs",
    "matchers.esnext",
    "matchers.d.ts"
  ],
  "module": "index.mjs",
  "esnext": "index.esnext",
  "exports": {
    "./": "./",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "esnext": "./index.esnext"
    },
    "./matchers": {
      "import": "./matchers.mjs",
      "require": "./matchers.js",
      "esnext": "./matchers.esnext"
    }
  }
}
